# with open(cert_path, 'rb') as f:
#     cert_data = f.read()
#     cert_base64 = base64.b64encode(cert_data).decode('utf-8')
# 
# # Ler e converter chave privada
# with open(key_path, 'rb') as f:
#     key_data = f.read()
#     key_base64 = base64.b64encode(key_data).decode('utf-8')
# 
# print("="*80)
# print("CERTIFICADOS CONVERTIDOS PARA BASE64")
