# =============================================================================
# EXEMPLO DE CONFIGURAÇÃO DE SECRETS PARA STREAMLIT CLOUD
# =============================================================================
# 
# INSTRUÇÕES:
# 1. Copie este conteúdo para Settings > Secrets no Streamlit Cloud
# 2. Preencha com as credenciais reais (NÃO commite secrets com credenciais reais!)
# 3. Para desenvolvimento local, copie para .streamlit/secrets.toml
#
# =============================================================================

# =============================================================================
# GITHUB - Configuração do Repositório
# =============================================================================
[github]
repo = "promettigustavo/dashboard-pipefy-kanastra"
branch = "main"

# =============================================================================
# PIPEFY - API Token
# =============================================================================
[pipefy]
api_token = "SEU_TOKEN_PIPEFY_AQUI"
# Exemplo: "eyJhbGciOiJIUzUxMiJ9.eyJpc3MiOiJQaXBlZnkiLCJpYXQiOjE3..."

# =============================================================================
# SANTANDER - Credenciais da API (Múltiplos Fundos)
# =============================================================================
# 
# OPÇÃO 1 (RECOMENDADA): Usar certificados .pem do repositório
# Faça upload dos arquivos santander_cert.pem e santander_key.pem para uma pasta
# 'certificados/' no repositório e configure os paths globais abaixo:
# 
# [santander_fundos]
# cert_path = "certificados/santander_cert.pem"
# key_path = "certificados/santander_key.pem"
# 
# OPÇÃO 2: Conteúdo PEM em secrets (se não puder fazer upload dos arquivos)
# Cole o conteúdo completo dos arquivos .pem diretamente em cada fundo:
# 
# cert_base64 = """-----BEGIN CERTIFICATE-----
# MIIFXzCCBEegAwIBAgIQAhG...
# ...
# -----END CERTIFICATE-----"""
# 
# key_base64 = """-----BEGIN PRIVATE KEY-----
# MIIEvgIBADANBgkqhkiG9w0...
# ...
# -----END PRIVATE KEY-----"""
# 
# =============================================================================

